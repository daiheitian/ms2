{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@": [
        "src"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@services": [
        "src/services"
      ],
      "@utils": [
        "src/utils"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  // "exclude": [
  //   "node_modules",
  //   "dist"
  // ]
}